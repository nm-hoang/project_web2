<%- include('header'); -%>

   <div class="row justify-content-center">
        <div class="col-8">
            <ul class="list-group">
                <% if (listUser) { %>
                    <% listUser.forEach(u => { %>
                        <li class="list-group-item d-flex justify-content-between align-items-center"> 
                            <%= u.email %> <br>
                            <%= u.displayName %> <br>
                            Account number: <%= u.account_number %> 
                            <a href="/conferencedetail/<%= u.id %>" class="btn btn-outline-secondary" type="button">Edit</a>
                            <a href="/admin" class="btn btn-outline-secondary" type="button">List attend</a>
                            
                        </li>
                        
                        <% }); %>
                        <% } %>
                </ul>
        </div>
    </div>
     


 <%- include('./footer'); -%>
