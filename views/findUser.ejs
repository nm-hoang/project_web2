<%- include('header'); -%>
<%- include('nav'); -%>
 <div class="content-body">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="card">
                            <div class="card-header border-0">
                                <h4 class="card-title">Users Lists</h4>
                            </div>
                            <div class="card-body pt-0">
                                <div class="transaction-table">
                                    <div class="table-responsive">
                                        <table class="table mb-0 table-responsive-sm">
                                         <thead>
                                          <tr>
                                            <th scope="col">#</th>
                                            <th scope="col"> Email</th>
                                            <th scope="col">Username</th>
                                            <th scope="col">Acount Number</th>
                                            <th scope="col">Acive</th>
                                          </tr>
                                        </thead>
                                        <tbody>
                                            <% var i = 1 ; users.forEach(function (t) { %>
                                              
                                            <tr>
                                                <td><%- i++ %> </td>
                                                <td><%= t.email %></td>
                                                <td><%= t.displayName %> </td>
                                                <td><%= t.account_number %> </td>
                                                <td class="text-danger"><%= t.active %> </td>
                                            </tr>
                                            <% }) %>
                                        </tbody>
            
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

     
<%- include('footer'); -%>