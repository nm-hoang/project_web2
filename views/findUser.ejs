<%- include('header'); -%>
<h1 >Users</h1>
<table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Email</th>
        <th scope="col">Username</th>
        <th scope="col">Acount Number</th>
        <th scope="col">Acive</th>
      </tr>
    </thead>
    <tbody>
        <% var i = 1 ; users.forEach(function (t) { %>
          
        <tr>
            <td><%- i++ %> </td>
            <td><%= t.email %></td>
            <td><%= t.displayName %> </td>
            <td><%= t.account_number %> </td>
            <td><%= t.active %> </td>
        </tr>
        <% }) %>
    </tbody>
</table>

     
<%- include('footer'); -%>